sf::RectangleShape GUI::drawWaitingScreen() {
	screen.setFillColor(sf::Color::Magenta);
	return screen;
}

sf::RectangleShape GUI::drawErrorScreen() {
	screen.setFillColor(sf::Color::Transparent);
	return screen;
}

RectangleShape GUI::drawBackground()
{
    screen.setFillColor(sf::Color::Transparent);
	return screen;
}

